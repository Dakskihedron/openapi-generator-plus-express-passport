{
  "name": "@openapi-generator-plus/typescript-express-passport-server-generator",
  "version": "1.5.3",
  "description": "An OpenAPI Generator+ module for an Express + Passport API server in TypeScript",
  "keywords": [
    "openapi-generator-plus",
    "openapi-generator-plus-generator",
    "openapi",
    "openapi-generator",
    "typescript",
    "express",
    "passport",
    "server"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "clean": "rimraf dist",
    "link-openapi-generator-plus": "npm link openapi-generator-plus $(cd node_modules && find @openapi-generator-plus -type d -depth 1 -prune) @openapi-generator-plus/typescript-express-passport-server-generator",
    "prepublish": "npm run build",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "author": "Karl von Randow",
  "license": "Apache-2.0",
  "dependencies": {
    "@openapi-generator-plus/handlebars-templates": "^0.14.1",
    "@openapi-generator-plus/indexed-type": "^0.1.5",
    "@openapi-generator-plus/typescript-generator-common": "^0.19.0"
  },
  "peerDependencies": {
    "@openapi-generator-plus/core": "^0.29.1"
  },
  "devDependencies": {
    "@openapi-generator-plus/core": "^0.29.1",
    "@openapi-generator-plus/generator-common": "^0.14.1",
    "@openapi-generator-plus/testing": "^0.29.1",
    "@openapi-generator-plus/types": "^0.29.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
